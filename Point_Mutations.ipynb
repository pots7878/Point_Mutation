   "source": [
    "def setSimpleRateMatrix(mu, dim):\n",
    "    a = np.empty([dim, dim])\n",
    "    a.fill(mu)\n",
    "    np.fill_diagonal(a, 1.0-(dim-1)*mu)\n",
    "    return a "
   ]
